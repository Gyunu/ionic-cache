  "bugs": {
    "url": "https://github.com/Gyunu/ng-cache/issues"
  },
  "homepage": "https://github.com/Gyunu/ng-cache#readme",
  "devDependencies": {
    "@angular/compiler": "^4.1.0",
    "@angular/compiler-cli": "^4.1.0",
    "rollup": "^0.41.6",
    "typescript": "^2.3.1",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {}
}
